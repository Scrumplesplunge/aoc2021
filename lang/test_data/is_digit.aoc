function strlen(message: []byte): int64 {
  var i: int64;
  while message[i] {
    i = i + 1;
  }
  return i;
}

function die(message: []byte): void {
  write(2, message as *void, strlen(message));
  write(2, "\n" as *void, 1);
  exit(1);
}

function isDigit(x: byte): int64 {
  return '0' <= x && x <= '9';
}

function main(): int64 {
  if isDigit('a') { die("bad"); }
  if !isDigit('0') { die("bad"); }
  return 0;
}
